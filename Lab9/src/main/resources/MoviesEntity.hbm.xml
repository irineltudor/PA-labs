<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lab9.entities.MoviesEntity" table="movies" schema="lab8java">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="releaseDate">
            <column name="release_date" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="score">
            <column name="score" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>